@page "/customer"
@model CustomerModel
@{
    ViewData["Title"] = "End to End Quoting";
}
<h1>Customer - </h1>
<p><button id="btn_new_quote">New Quote</button></p>
<table style="display: none;">
    <thead>
        <tr>
            <th>Date</th>
            <th>Variation</th>
            <th>Type</th>
            <th>(Cheapest) Carrier</th>
            <th>(Cheapest) Name</th>
            <th>(Cheapest) Premium</th>
        </tr>
    </thead>
    <tbody></tbody>
</table>

@section Scripts {
    <script>
        window.CUSTOMER_ID = "@Model.CustomerId";
    </script>

    <script src="~/js/customer.js"></script>
}